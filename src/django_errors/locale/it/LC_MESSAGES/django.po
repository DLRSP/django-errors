#: templates/base.html:9
msgid "An app to manage errors page ..."
msgstr "Una app per gestire le pagine di errorre ..."

#: templates/django_errors/400.html:5 templates/django_errors/400.html.py:9
msgid "Bad Request"
msgstr "Brutta Richiesta"

#: templates/django_errors/400.html:12
msgid "The request could not be understood by the server due to malformed syntax."
msgstr "La richiesta non può essere compresa dal server a causa di sintassi errata."

#: templates/django_errors/400.html:16
msgid "The browser SHOULD NOT repeat the request without modifications."
msgstr "Il browser NON DEVE ripetere la richiesta senza modifiche."

#: templates/django_errors/400.html:24 templates/django_errors/403.html:20
#: templates/django_errors/404.html:20 templates/django_errors/500.html:20
msgid "Details"
msgstr "Details"

#: templates/django_errors/403.html:5 templates/django_errors/403.html.py:9
msgid "Permission Denied"
msgstr "Permesso Negato"

#: templates/django_errors/403.html:12
msgid "You don't have permissions to see this page!"
msgstr "Non hai il permesso per vedere questa pagina!"

#: templates/django_errors/404.html:5 templates/django_errors/404.html.py:9
msgid "Page Not Found"
msgstr "Pagina non trovata"

#: templates/django_errors/404.html:12
msgid "I'm sorry, we don't have a page with that URL."
msgstr "Mi dispiace, non abbiamo una pagina con questo URL."

#: templates/django_errors/500.html:5
msgid "Internal Server Error"
msgstr "Errore Interno del Server"

#: templates/django_errors/500.html:9
msgid "Server Error"
msgstr "Errore del Server "

#: templates/django_errors/500.html:12
msgid "I'm sorry, an error has occurred with the application."
msgstr "Mi dispiace, si è verificato un errore con il programma."

